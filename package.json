{
  "name": "simply-paging",
  "version": "0.1.1",
  "description": "A simple ReactJS component that adds paging functionality to your project",
  "main": "dist/index.js",
  "scripts": {
    "bounce": "rm -rf node_modules;yarn",
    "build:component": "babel --minified ./src --out-dir ./build",
    "build:component:dev": "babel ./src --out-dir ./build",
    "build:css": "sass -s compressed ./src/scss/master.scss ./dist/css/master.min.css",
    "build:dev": "yarn build:css && yarn build:component:dev && yarn copytodist",
    "build": "yarn build:css && yarn build:component && yarn copytodist",
    "copytodist": "copyfiles -u 1 build/* build/components/SimplyPaging/* src/scss/* dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mariolo1985/Simply-Paging.git"
  },
  "keywords": [
    "carousel",
    "javascript",
    "next",
    "paging",
    "previous",
    "react",
    "ReactJS"
  ],
  "author": "Mario Lo <hello@websitesbymario.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mariolo1985/Simply-Paging/issues"
  },
  "homepage": "https://github.com/mariolo1985/Simply-Paging#readme",
  "dependencies": {
    "react": "^16.5.1",
    "prop-types": "^15.6.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "copyfiles": "^2.1.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-defaults": "^9.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.10.0"
  }
}
