{
  "name": "simply-paging",
  "version": "0.2.2",
  "description": "A vertical focused ReactJS carousel",
  "main": "dist/index.js",
  "scripts": {
    "build": "SET NODE_ENV=production && yarn build:component && grunt prod",
    "build:component": "babel --minified ./src --out-dir ./build",
